{
  "name": "moonyetis-frontend",
  "version": "2.0.0",
  "description": "MoonYetis Ecosystem Frontend - Modern crypto gaming platform on Fractal Bitcoin",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,ts,jsx,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,ts,jsx,tsx --fix",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "npx vite-bundle-analyzer"
  },
  "dependencies": {
    "gsap": "^3.12.2",
    "pixi.js": "^6.5.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-legacy": "^5.2.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "postcss": "^8.4.32",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": [
    "crypto",
    "gaming",
    "fractal-bitcoin",
    "brc-20",
    "coin-flip",
    "defi",
    "web3"
  ],
  "author": "MoonYetis Team",
  "license": "MIT"
}